@List empAvail = (List<Shift_Scheduler.Models.Employee>)ViewData["EmpAvailable"];
        <h2>ShiftChangeRequest</h2>
        <hr />
        @{Html.RenderPartial("_navbar"); }

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-horizontal">

                <div class="form-group">
                    <label class="control-label col-md-2">Current Shift:</label>
                    <div class="col-md-10">
                        <p>@ViewData["ShiftToChange"]</p>
                    </div>
                </div>

                <div class="form-group">
                    <label class="control-label col-md-2">Available Employees:</label>
                    <div class="col-md-10">
                        @Html.DropDownListFor(x => x.selectedEmployee ,(List<Shift_Scheduler.Models.Employee>)ViewData["EmpAvailable"])


                    </div>
                </div>

                <div class="form-group">
                    @Html.LabelFor(model => model.shiftScheduleID, "shiftScheduleID", htmlAttributes: new { @class = "control-label col-md-2" })
                    <div class="col-md-10">
                        @Html.DropDownList("Available Employees", null, htmlAttributes: new { @class = "form-control" })
                        @Html.ValidationMessageFor(model => model.shiftScheduleID, "", new { @class = "text-danger" })
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-md-offset-2 col-md-10">
                        <input type="submit" value="Create" class="btn btn-default" />
                    </div>
                </div>
            </div>
        }

        <div>
            @Html.ActionLink("Back to List", "Index")
        </div>
